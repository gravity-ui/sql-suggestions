{
  "name": "websql-autocomplete-2",
  "version": "0.3.3",
  "description": "A library that provides autocompletion and errors highlighting for various database dialects",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gravity-ui/websql-autocomplete-2"
  },
  "license": "Apache-2.0",
  "type": "module",
  "files": [
    "dist/*"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=16.0"
  },
  "scripts": {
    "generate": "antlr4ng -Dlanguage=TypeScript -o src/parsers/mysql/generated/ -visitor -listener -Xexact-output-dir src/parsers/mysql/grammar/MySqlLexer.g4 src/parsers/mysql/grammar/MySqlParser.g4",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "autocomplete": "node dist/index.js",
    "parse": "npm run build ; npm run autocomplete"
  },
  "devDependencies": {
    "@gravity-ui/eslint-config": "^3.1.1",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@gravity-ui/tsconfig": "^1.0.0",
    "antlr4ng-cli": "^1.0.7",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "antlr4-c3": "^3.3.5",
    "antlr4ng": "^2.0.5"
  }
}